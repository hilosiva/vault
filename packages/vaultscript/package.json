    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@types/node": "^22.2.0",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "@types/jsdom": "^21.1.7",
