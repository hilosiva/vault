  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "author": "Shibata Hironori",
  "keywords": [
    "JavaScript",
    "JavaScript Library"
  ],
  "repository": {
    "type": "git",
